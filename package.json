{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:all": "run-p dev dev:main dev:minor",
    "build:all": "run-p build build:main build:minor",
    "preview:all": "run-p preview preview:main preview:minor",
    "dev": "vue-tsc && vite",
    "dev:main": "vue-tsc && vite --mode main",
    "dev:minor": "vue-tsc && vite --mode minor",
    "build": "vue-tsc && vite build",
    "build:main": "vue-tsc && vite build --mode main",
    "build:minor": "vue-tsc && vite build --mode minor",
    "preview": "vite preview --open --outDir dist/master",
    "preview:main": "vite preview --open --outDir dist/main",
    "preview:minor": "vite preview --open --outDir dist/minor",
    "type-check": "vue-tsc",
    "eslint": "eslint . --ext .vue,.js,.ts --fix",
    "stylelint": "stylelint \"./**/*.{css,less,vue,html}\" --fix",
    "prettier": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\""
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "mitt": "^3.0.0",
    "pinia": "^2.0.36",
    "vue": "^3.3.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.0",
    "@vitejs/plugin-legacy": "^3.0.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.39.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.11.0",
    "less": "^4.2.0",
    "less-loader": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.8",
    "stylelint": "^15.10.2",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-less": "^1.0.8",
    "stylelint-order": "^6.0.3",
    "typescript": "~5.0.4",
    "unplugin-vue-components": "^0.22.12",
    "vconsole": "^3.15.1",
    "vite": "^4.3.9",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-vconsole": "^2.0.1",
    "vue-tsc": "^1.6.5"
  }
}